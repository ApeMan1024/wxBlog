.view {
    display: flex;
    flex-direction: column;
    margin: 10rpx;
}

.vi1 {
    display: flex;
    flex-direction: row;
    border: 1px solid #999;
    align-items: center;
    border-radius: 12rpx;
}

.vi1 input {
    flex: 8;
    padding-left: 5rpx;
}

.vi1 button {
    flex: 2;
}

.vi2 {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin: 20rpx 0;
    border-top: 2rpx dashed #999;
    padding-top: 10rpx;
}
.vi2 navigator{
    margin:10rpx 0;
} 
.vi2 .vie {
    display: flex;
    width: 98vw;
    height: 200rpx;
    padding-bottom: 10rpx;
    border-bottom: 1px dashed #999;
}

.vi2 .vie .vie_1 {
    width: 200rpx;
    height: 200rpx;
}

.vi2 .vie .vie_1 .img1 {
    width: inherit;
    height: inherit;
    border-radius: 12rpx;
}

.vi2 .vie .vie_2 {
    display: flex;
    flex-direction: column;
    margin-left: 10rpx;
}

.vi2 .vie .vie_2 .li {
    margin: 10rpx;
}

.vi2 .vie .vie_2 .li .text1 {
    font-weight: bold;
    color: #096;
}
.vi2 .vie .vie_2 .li .text3{
    color:#096;
    font-size: 1rem;
}
.vi2 .vie .vie_2 .li.li_1{
    display: flex;
    align-items: center;
    color:#096;
    justify-content: space-between;
}
.vi2 .vie .vie_2 .li_1 .img2{
    width:50rpx;
    height:50rpx;
}
.vi2 .vie .vie_2 .li_1 .li1{
    display: flex;
    align-items: center;
}
.vi2 .vie .vie_2 .li_1 .li1 text{
    margin-left:10rpx;
}
.vi3{
    display: flex;
    flex-direction: column;
    width:96vw;
    margin:auto;
    box-sizing: border-box;
    padding:10rpx;
    box-shadow: 1px 1px 8px #ccc;
    min-height:30vh;
}
.vi3 .vie1{
    padding-bottom: 5rpx;
    border-bottom:3rpx solid ;
}
.vi3 .vie2{
    padding-top:10rpx;
    display: flex;
    flex-wrap:wrap;
    justify-content: space-between;
}
.vi3 .vie2 navigator{
    background:red;
    width:200rpx;
    height:60rpx;
    margin:10rpx;
    text-align: center;
    line-height: 60rpx;
    color:#fff;
}
.vi4{
    width:96vw;
    height:30vh;
    margin:auto;
    margin-top:10rpx;
    position: relative;
    perspective: 100vw;
    transform-style: preserve-3d;
}
.vi4 .vie1{
    width:80vw;
    height:30vh;
    margin:auto;
    position: relative;
    transform-style: preserve-3d;
    transform-origin: center center 0;
    animation: key1 6s linear infinite;
}
.vi4 .vie1 .box1{
    width:20vw;
    height:20vw;
    position: absolute;
    margin:-10vw 0 0 -10vw;
    left:50%;
    top:50%;
    line-height: 20vw;
    text-align: center;
    transform-style: preserve-3d;
    color:#fff;
}
@keyframes key1{
    0%{
        transform:rotateY(0deg);
    }
    100%{
        transform: rotateY(-360deg);
    }
}
.vi4 .vie1 .box1:nth-child(1){
    background:maroon;
    transform:translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(2){
    background:mediumaquamarine;
    transform: rotateY(45deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(3){
    background:mediumorchid;
    transform: rotateY(90deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(4){
    background:moccasin;
    transform: rotateY(135deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(5){
    background:#C6E2FF;
    transform: rotateY(180deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(6){
    background:#C7C7C7;
    transform: rotateY(225deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(7){
    background:midnightblue;
    transform: rotateY(270deg) translateZ(30vw);
}
.vi4 .vie1 .box1:nth-child(8){
    background:lawngreen;
    transform: rotateY(315deg) translateZ(30vw);
}
